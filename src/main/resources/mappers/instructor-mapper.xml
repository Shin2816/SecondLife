<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 해당 파일에 모든 쿼리문을 작성 -->
<mapper namespace="">
	<resultMap id="" type="">
        <result property="INSTRUCTOR_CODE"      column="instructorCode"/>
        <result property="INSTRUCTOR_MAJOR"     column="instructorMajor"/>
        <result property="INSTRUCTOR_NAME"      column="instructorName"/>
        <result property="INSTRUCTOR_AGE"       column="instructorAge"/>
        <result property="INSTRUCTOR_GENDER"    column="instructorGender"/>
        <result property="INSTRUCTOR_PHONE"     column="instructorPhone"/>
        <result property="INSTRUCTOR_ADDR"      column="instructorAddr"/>
        <result property="HIREDATE"             column="hiredate"/>
    </resultMap>
</mapper>































